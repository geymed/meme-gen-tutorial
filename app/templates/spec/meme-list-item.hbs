<div class="panel panel-default">
	<div class="panel-heading"><h3 class="panel-title">meme-list-item Component</h3></div>
	<div class='panel-body'>
		{{#element-highlighter data=highlightData.ownedByAnother}}
			<div class="panel panel-default">
				<div class="panel-heading"><h3 class="panel-title">Does not belong to current user; Not liked by current user</h3></div>
				<div class='panel-body'>
					<div class='row'>
						<div class="col-xs-6 frame" style="zoom:0.7;">
							{{partial 'frames/meme-list-item-of-another'}}
						</div>
						<div class="col-xs-6">
							{{code-snippet name="meme-list-item-of-another.html" indent=false language='html'}}
						</div>
					</div>
				</div>
			</div>
		{{/element-highlighter}}
		{{#element-highlighter data=highlightData.ownedByCurrentUser}}
			<div class="panel panel-default">
				<div class="panel-heading"><h3 class="panel-title">Belongs to current user; Not liked by current user</h3></div>
				<div class='panel-body'>
					<div class='row'>
						<div class="col-xs-6 frame" style="zoom:0.7;">
							{{partial 'frames/meme-list-item'}}
						</div>
						<div class="col-xs-6">
							{{code-snippet name="meme-list-item.html" indent=false language='html'}}
						</div>
					</div>
				</div>
			</div>
		{{/element-highlighter}}
		{{#element-highlighter data=highlightData.ownedByCurrentUser}}
			<div class="panel panel-default">
				<div class="panel-heading"><h3 class="panel-title">Belongs to current user; Liked by current user</h3></div>
				<div class='panel-body'>
					<div class='row'>
						<div class="col-xs-6 frame" style="zoom:0.7;">
							{{partial 'frames/meme-list-item-liked'}}
						</div>
						<div class="col-xs-6">
							{{code-snippet name="meme-list-item-liked.html" indent=false language='html'}}
						</div>
					</div>
				</div>
			</div>
		{{/element-highlighter}}
	</div>
</div>