<div class="panel panel-default">
	<div class="panel-heading"><h3 class="panel-title">user-filter-toggler Component</h3></div>
	<div class='panel-body'>
		{{#element-highlighter data=highlightData.userFilterToggler}}
			<div class="panel panel-default">
				<div class="panel-heading"><h3 class="panel-title">Not Selected</h3></div>
				<div class='panel-body'>
					<div class='row'>
						<div class="col-xs-6 frame">
							{{partial 'frames/user-filter-toggler'}}
						</div>
						<div class="col-xs-6">
							{{code-snippet name="user-filter-toggler.html" indent=false language='html'}}
						</div>
					</div>
				</div>
			</div>
		{{/element-highlighter}}
		{{#element-highlighter data=highlightData.userFilterToggler}}
			<div class="panel panel-default">
				<div class="panel-heading"><h3 class="panel-title">Selected</h3></div>
				<div class='panel-body'>
					<div class='row'>
						<div class="col-xs-6 frame">
							{{partial 'frames/user-filter-toggler-selected'}}
						</div>
						<div class="col-xs-6">
							{{code-snippet name="user-filter-toggler-selected.html" indent=false language='html'}}
						</div>
					</div>
				</div>
			</div>
		{{/element-highlighter}}
	</div>
</div>